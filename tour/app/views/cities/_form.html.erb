<%= form_with(model: [ @country, @country.cities.build ], local: true) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <%= f.hidden_field :country_id, :value => params["id"] %>

  <%= f.submit %>
<% end %>